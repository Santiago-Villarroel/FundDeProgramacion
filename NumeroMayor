#include"stdafx.h"
#include <iostream>
#include"conio.h"

using namespace std;

void main()
{
    int numero1,numero2,numero3;
    cout<<"ingrese el primer numero: "; cin>>numero1;
    cout<<"ingrese el segundo numero: "; cin>>numero2;
    cout<<"ingrese el tercer numero: "; cin>>numero3;
    if (numero1>=numero2)
       if (numero1>=numero3)
	       cout<<"El mayor es: "<<numero1<<endl;
	   else 
		   cout<<"El mayor es: "<<numero3<<endl;
	else
	   if (numero2>=numero3)
		   cout<<"El mayor es: "<<numero2<<endl;
	   else 
		   cout<<"El mayor es: "<<numero3<<endl;
	getch();
}
